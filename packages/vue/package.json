{
  "name": "@efie-form/vue",
  "version": "0.2.0",
  "description": "Vue components for Efie Form",
  "type": "module",
  "license": "MIT",
  "author": "Wei Kang",
  "homepage": "https://github.com/pwkang/efie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/efie-form/efie.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/pwkang/efie/issues"
  },
  "keywords": [
    "vue",
    "form",
    "form-builder",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@efie-form/core": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
